#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int amtgiven;
    scanf("%d",&amtgiven);
    int billamt;
    scanf("%d",&billamt);
    printf("Quotient:%d\nRemainder:%d",(amtgiven/billamt),(amtgiven%billamt));
    return 0;
}
